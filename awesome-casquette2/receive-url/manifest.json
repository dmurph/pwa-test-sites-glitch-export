{
  "name": "Receive URL",
  "icons": [
    {
      "src": "https://cdn.glitch.com/9bdcb1a9-d93f-4d1a-9d46-be446fbc251f%2Fblue-192.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "start_url": "index.html",
  "display": "minimal-ui",
  "share_target": {
    "action": "share-target-destination.html",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "received_title",
      "url": "received_url",
      "files": [
        {
          "name": "received_image_files",
          "accept": [
            ".png", "image/png",
            ".svg", "image/svg+xml",
            ".webp", "image/webp"
          ]
        }
      ]
    }
  }
}
