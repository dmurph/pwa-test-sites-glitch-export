{
  "name": "Partial Wild Image Target",
  "theme_color": "orange",
  "icons": [
    {
      "src": "https://cdn.glitch.com/9bdcb1a9-d93f-4d1a-9d46-be446fbc251f%2Fblue-192.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "start_url": "index.html",
  "display": "minimal-ui",
  "display_override": [ "tabbed" ],
  "tab_strip": {
    "home_tab": {
      "url": "index.html",
      "icons": [
        {"src":"gold.svg","type":"image/svg+xml","sizes":"64x64"}
      ]
    },
    "new_tab_button": "auto"
  },
  "share_target": {
    "action": "share-target-destination.html",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "received_image_files",
          "accept": [
            "image/*"
          ]
        }
      ]
    }
  }
}

