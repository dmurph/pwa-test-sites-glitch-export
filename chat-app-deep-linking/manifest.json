{
  "name": "Fake Chat App",
  "short_name": "Fake Chat",
  "start_url": "/",
  "id": "/",
  "theme_color": "#000",
  "background_color": "#000",
  "icons": [
    {
      "src": "https://cdn.glitch.global/af8cbd49-47de-44ef-947f-50ea0f83a60c/fake-chat-app-icon.png?v=1737144310386",
      "type": "image/png",
      "sizes": "257x257"
    }
  ],
  "display": "minimal-ui",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "file_handlers": [
    {
      "action": "/",
      "name": "Send Chat Message",
      "accept": {
        "text/plain": [".fakechat"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+fakechat",
      "url": "/?open-from-protocol=%s"
    }
  ]
}
