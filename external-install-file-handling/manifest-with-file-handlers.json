{
  "name": "External Install File Handling Tester",
  "display": "standalone",
  "start_url": "/index.html",
  "icons": [
    {
      "src": "https://cdn.glitch.global/cd750581-1811-4b20-add6-1fb0cc5b406b/ai-icon.png?v=1663962546607",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "file_handlers": [
    {
      "action": "/index.html?plaintext",
      "name": "Plain Text",
      "accept": {
        "text/plain": [
          ".txt",
          ".md",
          ".csv",
          ".text",
          ".foo",
          ".bar",
          ".foobar",
          ".turbotax"
        ]
      },
      "icons": [
        {
          "src": "https://cdn.glitch.global/cd750581-1811-4b20-add6-1fb0cc5b406b/ai-icon.png?v=1663962546607",
          "sizes": "512x512"
        }
      ]
    }
  ]
}
